{
  "name": "@lab009/erebus",
  "version": "2.0.1",
  "description": "Dead-simple API actions for Redux",
  "repository": {
    "type": "git",
    "url": "https://github.com/lab009/erebus.git"
  },
  "author": "Oleg Orlov <mail@orlovoleg.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "es"
  ],
  "scripts": {
    "preversion": "npm run clean && npm run check && npm run build",
    "clean": "rimraf dist es",
    "lint": "eslint src",
    "test": "cross-env BABEL_ENV=commonjs ava",
    "check": "npm run lint && npm run test",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir dist",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:commonjs && npm run build:es"
  },
  "dependencies": {
    "immutable": "3.8.1",
    "lodash.mapvalues": "4.6.0",
    "lodash.merge": "4.6.0",
    "normalizr": "2.2.1",
    "reduce-reducers": "0.1.2",
    "redux-actions": "1.2.0",
    "superagent": "3.3.2"
  },
  "devDependencies": {
    "@lab009/babel-preset-lab009": "2.0.0",
    "@lab009/eslint-config-lab009": "2.1.0",
    "ava": "0.17.0",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-register": "6.22.0",
    "cross-env": "3.1.4",
    "eslint": "3.13.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.9.0",
    "rimraf": "2.5.4"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  }
}
